{
  "name": "sweepy",
  "version": "1.1.0",
  "description": "AST-powered CLI to detect and clean unused JS/TS modules, CSS/SCSS stylesheets, selectors, exports, .env keys, and orphaned assets like PNG/JPG/SVG. Keep your codebase clean and optimized.",
  "bin": {
    "sweepy": "./index.js"
  },
  "type": "module",
  "main": "./index.js",
  "files": [
    "index.js",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "cli",
    "cli-tool",
    "code-cleaner",
    "codebase-cleaner",
    "dead-code",
    "dead-code-removal",
    "unused-files",
    "unused-modules",
    "orphaned-assets",
    "orphaned-files",
    "js-unused",
    "css-unused",
    "asset-cleanup",
    "static-assets",
    "typescript",
    "javascript",
    "scss",
    "css",
    "svg",
    "jpg",
    "png",
    "jpeg",
    "media-detector",
    "codebase-optimizer",
    "import-graph",
    "dynamic-import",
    "require-tracker",
    "tree-shaking",
    "modularization",
    "linting",
    "devtools",
    "frontend-tools",
    "developer-cli",
    "dependency-visualizer",
    "code-quality",
    "modern-tooling",
    "esbuild",
    "vite",
    "rollup",
    "webpack",
    "project-audit",
    "file-analyzer",
    "build-optimizer",
    "automation",
    "clean-architecture",
    "refactor-tools",
    "zero-dependency-cleaner",
    "maintenance-tool",
    "unused-cleanup",
    "build-artifacts",
    "code-debloat",
    "unused-exports",
    "export-cleaner",
    "dead-exports",
    "symbol-analysis",
    "tree-shake",
    "export-tracker"
  ],
  "author": "anthonyhalim150",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonyhalim150/sweepy"
  },
  "bugs": {
    "url": "https://github.com/anthonyhalim150/sweepy/issues"
  },
  "homepage": "https://github.com/anthonyhalim150/sweepy#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "globby": "^13.2.2",
    "inquirer": "^12.5.2",
    "postcss": "^8.5.3",
    "postcss-selector-parser": "^7.1.0"
  },
  "scripts": {
    "test": "vitest run"
  },
  "devDependencies": {
    "vitest": "^3.1.1"
  },
  "sweepy": {
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/coverage/**",
      "**/.next/**",
      "**/.vercel/**",
      "**/.turbo/**",
      "**/.parcel-cache/**",
      "**/.cache/**",
      "**/.vite/**",
      ".sweepy-trash/**",
      "**/test/**",
      "**/tests/**",
      "**/__tests__/**",
      "**/__mocks__/**",
      "**/*.spec.*",
      "**/*.test.*",
      "**/*.snap",
      "**/*.log",
      "**/*.tmp",
      "**/.sweepy-report.*",
      "**/package-lock.json",
      "**/pnpm-lock.yaml",
      "**/yarn.lock",
      "**/cypress/**",
      "**/playwright/**",
      "**/.eslintcache",
      "**/.stylelintcache",
      "**/.DS_Store",
      "**/Thumbs.db"
    ],
    "output": "json",
    "types": [
      "js",
      "css",
      "assets"
    ]
  }
}
